    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: dev",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}\\fullstack-frontend"
        },
        {
            "type": "java",
            "name": "Spring Boot-FullstackBackendApplication<Fullstack-backend>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.Songz.Fullstack_backend.FullstackBackendApplication",
            "projectName": "Fullstack-backend",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}